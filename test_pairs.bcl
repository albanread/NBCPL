// Simple PAIR test for NewBCPL
LET START() BE {

    WRITEF("Testing PAIR functionality...")

    LET p1 = PAIR(10, 20)
    LET p2 = PAIR(30, 15)
  
	
    WRITEF("*Np1 is a %s value %P *N ", TYPE(p1), p1)

    WRITEF("*Np2 is a %s value %P *N ", TYPE(p2), p2)

    LET p3 = p1 + p2 
    WRITEF("*Np3 is a %s value %P *N ", TYPE(p3), p3)

}
