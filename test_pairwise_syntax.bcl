LET START() BE {
    // Test 1: PAIRWISE_MIN with two arguments
    LET a = PAIRWISE_MIN(data1, data2)

    // Test 2: PAIRWISE_MAX with two arguments
    LET b = PAIRWISE_MAX(vec1, vec2)

    // Test 3: PAIRWISE_ADD with two arguments
    LET c = PAIRWISE_ADD(floats1, floats2)

