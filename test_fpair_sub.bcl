// Test file for verifying FPAIR 2S dedicated encoders
// Use --list to see clang assembly/hex and --tracecodegen to see JIT encodings
// Then grep for the hex values to compare

LET START() BE {
    WRITEF("Testing FPAIR 2S dedicated encoders*N")
    // Test FPAIR subtraction (should use fsub_vector_2s)
    LET d = FPAIR(10.0, 8.0)
    LET e = FPAIR(3.0, 2.0)
    LET f = d - e
    WRITEF("FPAIR subtraction: %Q - %Q = %Q*N", d, e, f)
   
}
