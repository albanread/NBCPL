LET START() BE
{
    WRITEF("Testing Integer PAIR Arithmetic Operations*N")

    // Create test PAIR values
    LET p1 = PAIR(12, 24)
    LET p2 = PAIR(3, 4)

    WRITEF("Test PAIRs: p1 = PAIR(12, 24), p2 = PAIR(3, 4)*N")

    // Test 1: Addition (should work)
    WRITEF("*NTest 1: Addition*N")
    LET add_result = p1 + p2
    WRITEF("p1 + p2 = PAIR(%d, %d)*N", add_result.|0|, add_result.|1|)
    WRITEF("Expected: PAIR(15, 28)*N")

    // Test 2: Subtraction (should work based on code analysis)
    WRITEF("*NTest 2: Subtraction*N")
    LET sub_result = p1 - p2
    WRITEF("p1 - p2 = PAIR(%d, %d)*N", sub_result.|0|, sub_result.|1|)
    WRITEF("Expected: PAIR(9, 20)*N")

    // Test 3: Multiplication (should work based on code analysis)
    WRITEF("*NTest 3: Multiplication*N")
    LET mul_result = p1 * p2
    WRITEF("p1 * p2 = PAIR(%d, %d)*N", mul_result.|0|, mul_result.|1|)
    WRITEF("Expected: PAIR(36, 96)*N")

    // Test 4: Division - check if supported
    WRITEF("*NTest 4: Division (may not be supported)*N")
    WRITEF("Based on code analysis, integer PAIR division is not supported*N")
    WRITEF("Error message: 'division not supported for integer PAIRs'*N")

    WRITEF("*NInteger PAIR operations test complete!*N")
}
