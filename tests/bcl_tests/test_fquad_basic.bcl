// Basic FQUAD test - testing simple 4x32-bit float vector operations

LET main() = VALOF {
    // Create an FQUAD with 4 float values
    LET fq1 = FQUAD(1.0, 2.0, 3.0, 4.0)
    LET fq2 = FQUAD(0.5, 1.5, 2.5, 3.5)

    WRITEF("Testing basic FQUAD creation and access...*N")

    // Test individual element access (only first two elements for now)
    WRITEF("fq1.first = %F*N", fq1.first)
    WRITEF("fq1.second = %F*N", fq1.second)

    WRITEF("fq2.first = %F*N", fq2.first)
    WRITEF("fq2.second = %F*N", fq2.second)

    // Test FQUAD arithmetic
    LET fq_result = fq1 + fq2

    WRITEF("After addition (fq1 + fq2):*N")
    WRITEF("Result.first = %F*N", fq_result.first)
    WRITEF("Result.second = %F*N", fq_result.second)

    WRITEF("FQUAD basic test completed successfully!*N")

    RESULTIS 0
}
