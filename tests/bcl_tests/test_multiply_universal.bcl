// Test multiplication operations in universal expressions
// This tests the new universal expression system for multiplication

LET START() BE {
    // Test 1: Float * int
    LET x = 1.5 * 2
    WRITEF("1.5 * 2 = %f*N", x)

    // Test 2: Int * float
    LET y = 3 * 2.0
    WRITEF("3 * 2.0 = %f*N", y)

    // Test 3: Pure integer multiplication
    LET z = 10 * 5
    WRITEF("10 * 5 = %d*N", z)

    // Test 4: Pure float multiplication
    LET w = 1.5 * 2.5
    WRITEF("1.5 * 2.5 = %f*N", w)

    // Test 5: Chained multiplication
    LET a = 2 * 3 * 4
    WRITEF("2 * 3 * 4 = %d*N", a)

    // Test 6: Mixed chained multiplication
    LET b = 2.0 * 3 * 4
    WRITEF("2.0 * 3 * 4 = %f*N", b)
}
