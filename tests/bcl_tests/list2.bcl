
LET START() BE
$(
  LET LS1 = LIST( "this", "is", "a", "test")
  LET LS2 = LIST( "the", "quick", "brown", "fox")
  LET LS3 = CONCAT(LS2, LS1)

  FOREACH T,S IN LS3 DO $(
	SWITCHON T INTO
	 $(
        	CASE TYPE_STRING:
		 $(
            		WRITES("String: ")
            		WRITES(AS_STRING(S))
            		WRITES("*N")
            		ENDCASE
        	$)
   	 $)

	WRITES("*N")

  $)

$)
