// Simple test to demonstrate REMANAGE semantic analysis tracing
LET START() BE $(
    WRITES("REMANAGE trace test starting...*N")

    LET x = VEC 10
    WRITES("Vector allocated*N")

    RETAIN x
    WRITES("Vector RETAINed*N")

    REMANAGE x
    WRITES("Vector REMANAGEd*N")

    WRITES("Test complete*N")
$)
