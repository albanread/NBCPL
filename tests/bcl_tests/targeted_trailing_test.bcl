   WRITES("VEC[] trailing comma: OK*N")

   // Test 4: TABLE expressions (confirmed working earlier)
   LET table1 = TABLE("i", "j")
   LET table2 = TABLE("i", "j",)
   WRITES("TABLE trailing comma: OK*N")

