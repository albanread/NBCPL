LET START() BE
$(
   LET S = "This is the age of the train"
   LET L = SPLIT(S," ")
   FOREACH T, V IN L DO
   $( 
      SWITCHON T INTO $( 
        CASE TYPE_STRING: $(
            WRITES("String: ")
            WRITES(AS_STRING(V))
            WRITES("*N")
            ENDCASE
        $)
     $)
   $)
$)
