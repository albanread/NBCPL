LET START() BE {
    WRITEF("Testing CFG-driven MIN operation*N")

    // Create simple PAIRS vectors
    LET a = PAIRS 2
    LET b = PAIRS 2

    // Initialize with simple values
    a!0 := PAIR(1, 2)
    a!1 := PAIR(5, 6)

    b!0 := PAIR(3, 1)
    b!1 := PAIR(4, 7)

    WRITEF("a: (1,2), (5,6)*N")
    WRITEF("b: (3,1), (4,7)*N")

    // This should trigger CFG-driven MIN reduction
    LET result = MIN(a, b)

    WRITEF("MIN result: %P, %P*N", result!0, result!1)
    WRITEF("Expected: (1,1), (4,6)*N")
}
