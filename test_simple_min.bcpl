GET "libhdr"

LET START() BE
{
    WRITEF("Simple MIN test*N")

    // Create small PAIRS vectors (4 elements each)
    LET vec1 = PAIRS 4
    LET vec2 = PAIRS 4

    WRITEF("Vectors allocated*N")

    // Fill with simple test values
    vec1!0 := 10
    vec1!1 := 20
    vec1!2 := 30
    vec1!3 := 40
    vec1!4 := 50
    vec1!5 := 60
    vec1!6 := 70
    vec1!7 := 80

    vec2!0 := 5
    vec2!1 := 25
    vec2!2 := 35
    vec2!3 := 15
    vec2!4 := 45
    vec2!5 := 55
    vec2!6 := 65
    vec2!7 := 75

    WRITEF("Values set*N")

    // Show input values
    WRITEF("vec1: (%N,%N), (%N,%N), (%N,%N), (%N,%N)*N",
           vec1!0, vec1!1, vec1!2, vec1!3, vec1!4, vec1!5, vec1!6, vec1!7)
    WRITEF("vec2: (%N,%N), (%N,%N), (%N,%N), (%N,%N)*N",
           vec2!0, vec2!1, vec2!2, vec2!3, vec2!4, vec2!5, vec2!6, vec2!7)

    WRITEF("Calling MIN...*N")
    LET result = MIN(vec1, vec2)
    WRITEF("MIN completed*N")

    // Show results
    WRITEF("Result: (%N,%N), (%N,%N), (%N,%N), (%N,%N)*N",
           result!0, result!1, result!2, result!3, result!4, result!5, result!6, result!7)
}
