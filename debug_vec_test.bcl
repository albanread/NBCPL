GET "libhdr.h"

LET START() = VALOF
$(
    WRITES("=== Vector Debug Test ===*N")

    // Test 1: Basic vector allocation
    WRITES("Test 1: Basic vector allocation*N")
    LET V = VEC 5

    IF V = 0 THEN
    $(
        WRITES("ERROR: Vector allocation failed*N")
        RESULTIS 1
    $)

    WRITES("Vector allocated successfully*N")

    // Test 2: Try to get length
    WRITES("Test 2: Getting vector length*N")
    LET LEN_V = LEN(V)
    WRITES("Length of V: ") WRITEN(LEN_V) WRITES("*N")

    // Test 3: Try basic assignment
    WRITES("Test 3: Basic vector assignment*N")
    V!0 := 42
    V!1 := 100

    WRITES("V!0 = ") WRITEN(V!0) WRITES("*N")
    WRITES("V!1 = ") WRITEN(V!1) WRITES("*N")

    WRITES("=== All tests completed ===*N")
    RESULTIS 0
$)
